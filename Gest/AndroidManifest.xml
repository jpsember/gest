<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.js.gest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <!-- Required for unit tests, in order to write snapshots to external storage;
    note, this is only necessary if some of the snapshots are actually changing.  Once
    they have been created and stored in the assets folder, this permission can be
    omitted. 
       <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />  
		-->
		
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
     	<activity android:name="com.js.gest.GestActivity" >
      		<intent-filter>
        		<action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
     	</activity>
    </application>

</manifest>

